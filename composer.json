    "autoload": {
        "psr-4": {
            "GhaniniaIR\\Interactive\\": "src/",
            "GhaniniaIR\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Amin Ghaninia",
            "email": "info@ghaninia.ir",
            "homepage": "https://ghaninia.ir"
        }
    ],
    "require": {
        "php": "^8.0" ,
        "laravel/framework" : "^9.0|^8.0|^7.0",
        "predis/predis": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GhaniniaIR\\Interactive\\InteractiveServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
